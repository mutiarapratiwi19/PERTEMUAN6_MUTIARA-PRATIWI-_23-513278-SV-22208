# Definisikan 4 himpunan
A = {100, 7, 8}
B = {200, 4, 5}
C = {300, 2, 3}
D = {100, 200, 300}

# Tampilkan hasil irisan
print("a. A∩D = ", A.intersection(D))
print("b. B∩D = ", B.intersection(D))
print("c. C∩D = ", C.intersection(D))
print("d. A∩B∩C∩D = ", A.intersection(B).intersection(C).intersection(D))
